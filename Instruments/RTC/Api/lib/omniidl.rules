<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="omniIDL compiler"
	Version="8.00"
	>
	<Rules>
		<CustomBuildRule
			Name="omniidl"
			DisplayName="omniidl"
			CommandLine="$(OMNIORB_ROOT)\bin\x86_win32\omniidl -nf -bcxx -Wba -Wbs=SK.cpp -Wbd=DynSK.cpp -Wbtf -I $(OMNIORB_ROOT)\idl -I $(OMNIORB_ROOT)\idl\COS -I .\idl [AllOptions] [Additionaloptions] &quot;$(InputPath)&quot;&#x0D;&#x0A;move $(InputName).hh .\include&#x0D;&#x0A;move $(InputName)SK.cpp .\src&#x0D;&#x0A;move $(InputName)DynSK.cpp .\src&#x0D;&#x0A;"
			Outputs=".\include\$(InputName).hh; .\src\$(InputName)SK.cpp; .\src\$(InputName)DynSK.cpp"
			FileExtensions="*.idl"
			ExecutionDescription="omniidl: compiling $(InputName)..."
			>
			<Properties>
				<StringProperty
					Name="include"
					DisplayName="Include path"
					Category="Includes"
					Description="These are the include path for the IDL compiler."
					Switch="-I [value]"
					DefaultValue="&quot;&quot;"
					Delimited="true"
					Inheritable="true"
				/>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
